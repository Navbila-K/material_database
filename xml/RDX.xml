<?xml version="1.0" encoding="UTF-8"?>

<!--
=========================================================
                MATERIAL DESCRIPTION FILE
=========================================================
This XML defines a single material along with:
- metadata (id, name, author, date, version)
- categories of physical models (thermal, mechanical, plasticity, reaction, EOS)
- data stored as entries with units separated into attributes
- fully explicit structure, future-proof and backward compatible
=========================================================
-->
<Material>
  <!-- Material metadata -->
  <Metadata>
    <Id>RDX-001</Id>
    <Name>RDX</Name>
    <Author></Author>
    <Date></Date>
    <Version meaning="schema_version">0.0.0-4753428F</Version>
  </Metadata>

  <Category>
    <Property>

      <!-- Phase information -->
      <Phase>
        <State>solid</State>
      </Phase>

      <!-- Thermal / mechanical intrinsic properties -->
      <Thermal>
        <!-- Specific heats -->
        <Cp unit="J/kg/K">
          <Entry ref="17">1590</Entry>
        </Cp>

        <Cv unit="J/kg/K">
          <Entry ref="9">1256</Entry>
        </Cv>
      </Thermal>

      <Mechanical>
        <!-- Density -->
        <Density unit="kg/m^3">
          <Entry ref="6">1806</Entry>
          <Entry ref="101">1798</Entry>
        </Density>

        <!-- No viscosity given -->
        <Viscosity unit="Pa*s">
          <Entry ref=""></Entry>
        </Viscosity>
      </Mechanical>

    </Property>

    <!-- ======================= MODEL SECTION ======================= -->
    <Model>
      <ElasticModel>
        <ThermoMechanical>

          <!-- Density used in model -->
          <Density>
            <Entry unit="kg/m3" ref="6">1806</Entry>
            <Entry unit="kg/m3" ref="101">1798</Entry>
            <Entry unit="kg/m3"></Entry>
          </Density>

          <!-- Ambient temperature -->
          <AmbientTemperature>
            <Entry unit="K" ref="7">298</Entry>
            <Entry unit="K" ref="57">477</Entry>
          </AmbientTemperature>

          <!-- Melting temperature -->
          <MeltingTemperature>
            <Entry unit="K" ref="7">478</Entry>
            <Entry unit="K" ref="87">474</Entry>
          </MeltingTemperature>

          <!-- Sound speed -->
          <SoundSpeed>
            <Entry unit="m/s" ref="8">2620</Entry>
          </SoundSpeed>

          <!-- Gruneisen coefficient -->
          <GruneisenCoefficient>
            <Entry unit="1" ref="8">0.618</Entry>
          </GruneisenCoefficient>

          <!-- Bulk modulus -->
          <IsothermalBulkModulus>
            <Entry unit="Pa" ref="81">13E9</Entry>
            <Entry unit="Pa" ref="98">13E9</Entry>
          </IsothermalBulkModulus>

          <!-- Cv again (EOS specific) -->
          <SpecificHeatIsochoric>
            <Entry unit="J/kg/K" ref="9">1256</Entry>
          </SpecificHeatIsochoric>

          <!-- Polynomial coefficients (empty) -->
          <SpecificHeatConstants>
            <c0 unit="J/kg/K"></c0>
            <c1 unit="J/kg/K"></c1>
            <c2 unit="J/kg/K"></c2>
            <c3 unit="J/kg/K"></c3>
          </SpecificHeatConstants>

          <!-- Cp -->
          <SpecificHeatIsobaric>
            <Entry unit="J/kg/K" ref="17">1590</Entry>
          </SpecificHeatIsobaric>

          <!-- Thermal conductivity -->
          <ThermalConductivity>
            <Entry unit="W/m/K" ref="17">23</Entry>
            <Entry unit="W/m/K" ref="57">0.26</Entry>
          </ThermalConductivity>

          <!-- Thermal expansion -->
          <ThermalExpansion>
            <Entry unit="1/K" ref="83">0.0000599</Entry>
          </ThermalExpansion>

        </ThermoMechanical>
      </ElasticModel>

      <ElastoPlastic>

        <!-- Shear modulus -->
        <ShearModulus>
          <Entry unit="Pa">1.00E10</Entry>
        </ShearModulus>

        <!-- Yield strength -->
        <YieldStrength>
          <Entry unit="Pa" ref="27">300</Entry>
          <Entry unit="Pa" ref="87">82</Entry>
        </YieldStrength>

        <!-- JC model (blank values) -->
        <JohnsonCookModelConstants>
          <StrainRate unit="1"></StrainRate>
          <ReferenceTemperature unit="K"></ReferenceTemperature>
          <A unit="Pa"></A>
          <B unit="Pa"></B>
          <n unit="1"></n>
          <C unit="1"></C>
          <M unit="1"></M>
          <MeltingTemperature unit="K"></MeltingTemperature>
          <ReferencePressure unit="Pa"></ReferencePressure>
          <A_prime unit="Pa"></A_prime>
          <C_prime unit="1"></C_prime>
        </JohnsonCookModelConstants>

      </ElastoPlastic>

      <!-- ===================== Reaction Model ===================== -->
      <ReactionModel>
        <Kind ref="57">3-step</Kind>

        <!-- Pre-exponential LnZ -->
        <LnZ>
          <Entry index="1" unit="1/s" ref="57">45.5</Entry>
          <Entry index="2" unit="1/s" ref="57">40.7</Entry>
          <Entry index="3" unit="1/s" ref="57">35</Entry>
          <Entry index="4" unit="1/s"></Entry>
        </LnZ>

        <!-- Activation energies -->
        <ActivationEnergy>
          <Entry index="1" unit="kcal/mol" ref="57">47.1</Entry>
          <Entry index="2" unit="kcal/mol" ref="57">44.1</Entry>
          <Entry index="3" unit="kcal/mol" ref="57">34.1</Entry>
          <Entry index="4" unit="kcal/mol"></Entry>
        </ActivationEnergy>

        <!-- Heat release -->
        <HeatRelease>
          <Entry index="1" unit="cal/g" ref="57">100</Entry>
          <Entry index="2" unit="cal/g" ref="57">-300</Entry>
          <Entry index="3" unit="cal/g" ref="57">-1200</Entry>
          <Entry index="4" unit="cal/g"></Entry>
        </HeatRelease>

        <!-- Single step parameters -->
        <ReactionModelParameter>
          <Steps unit="1"></Steps>

          <!-- Shared with Joseph -->
          <Ea unit="kcal/mol">36.75</Ea>
          <lnZ unit="1/s">37.22</lnZ>
          <Q_R unit="J/g">4689.44</Q_R>

          <!-- New -->
          <Ea_new unit="kcal/mol">36.75</Ea_new>
          <lnZ_new unit="1/s">37.22</lnZ_new>
          <Q_R_new unit="J/g">4689.44</Q_R_new>
          
          <!-- Old -->
          <Ea_old unit="kcal/mol">39.53</Ea_old>
          <lnZ_old unit="1/s">38.66</lnZ_old>
        </ReactionModelParameter>

      </ReactionModel>

      <!-- ======================= EOS ======================= -->
      <EOSModel>

        <!-- Generic Row 1 (BM) -->
        <Row index="1">
          <Kind></Kind>
          <K0 unit="Pa"></K0>
          <K0Prime></K0Prime>
        </Row>

        <!-- Generic Row 2 -->
        <Row index="2">
          <Kind></Kind>
          <KT0 unit="Pa"></KT0>
          <N unit="1"></N>
        </Row>

        <!-- Generic Row 3 -->
        <Row index="3">
          <Kind></Kind>
          <Rho unit="kg/m3"></Rho>
          <s unit="1"></s>
          <Cs unit="m/s"></Cs>
          <Gamma unit="1"></Gamma>
          <Cv unit="J/kg/K"></Cv>
        </Row>

        <!-- Generic Row 4 -->
        <Row index="4">
          <Kind></Kind>
          <GruneisenCoefficient unit="1"></GruneisenCoefficient>
          <A unit="GPa"></A>
          <B unit="GPa"></B>
          <C unit="GPa"></C>
          <D unit="GPa"></D>
          <Rho0 unit="kg/m3"></Rho0>
          <SpecificHeatCv unit="J/kg/K"></SpecificHeatCv>
        </Row>

        <!-- WSD-like row -->
        <Row index="5">
          <Kind></Kind>
          <a unit="1"></a>
          <n unit="1"></n>
          <Vc unit="cm3/g"></Vc>
          <Pc unit="Pa"></Pc>
          <b unit="1"></b>
          <Cv unit="J/kg/K"></Cv>
        </Row>

        <!-- JWL -->
        <Row index="6">
          <Kind ref="29">JWL</Kind>

          <unreacted>
            <A unit="GPa"></A>
            <B unit="GPa"></B>
            <C unit="MJ/kg"></C>
            <R1 unit="1"></R1>
            <R2 unit="1"></R2>
            <w unit="1"></w>
            <Cv unit="Mbar/K"></Cv>
            <E0 unit="Mbar"></E0>
          </unreacted>

          <reacted>
            <A unit="GPa" ref="30">828.1</A>
            <B unit="GPa" ref="30">10.527</B>
            <C unit="MJ/kg" ref="30">1.499</C>
            <R1 unit="1" ref="30">4.844</R1>
            <R2 unit="1" ref="30">1.066</R2>
            <w unit="1" ref="30">0.395</w>
            <Cv unit="Mbar/K" ref="30">1256</Cv>
            <E0 unit="Mbar"></E0>
          </reacted>
        </Row>

      </EOSModel>

    </Model>
  </Category>
</Material>
