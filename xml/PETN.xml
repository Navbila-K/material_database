<?xml version="1.0" encoding="UTF-8"?>

<!--
=========================================================
                MATERIAL DESCRIPTION FILE
=========================================================
This XML defines a single material along with:
- metadata (id, name, author, date, version)
- categories of physical models (thermal, mechanical, plasticity, reaction, EOS)
- data stored as entries with units separated into attributes
- fully explicit structure, future-proof and backward compatible
=========================================================
-->
<Material>
  <!-- The metadata identifies the material and makes files traceable. -->
  <Metadata>
  <Id>PETN-001</Id>
  <Name>PETN</Name>
  <Author></Author>
  <Date></Date>
  <Version meaning="schema_version">0.0.0-3697F06A</Version><!-- Indicates the version of the XML schema used for defining this material structure -->
  </Metadata>
    
  <Category>
    <Property>

            <!-- Phase information (enumeration: gas, liquid, solid, plasma) -->
            <Phase>
                <State>solid</State>
            </Phase>

            <!-- Thermal properties -->
            <Thermal>
                <!-- specific heat at constant pressure (J/kg·K) -->
                <Cp unit="J/kg/K">
                    <Entry ref="23">1080</Entry>
                </Cp>

                <!-- specific heat at constant volume (J/kg·K) -->
                <Cv unit="J/kg/K">
                    <Entry ref="22">1140</Entry>
                    <Entry ref="23">1090</Entry>
                </Cv>
            </Thermal>

            <!-- Mechanical properties -->
            <Mechanical>
                <!-- density in kg/m^3 -->
                <Density unit="kg/m^3">
                    <Entry ref="18">1778</Entry>
                    <Entry ref="23">1760</Entry>
                </Density>

                <!--viscosity in Pa·s -->
                <Viscosity unit="Pa*s"> <!-- or we can use cP (centipoise) as alt-->
                    <Entry ref="0">0.0000</Entry>
                </Viscosity>
            </Mechanical>

        </Property>

    <Model>
      <ElasticModel>
        <ThermoMechanical>
          <!-- Density values used in thermo-mechanical calculations -->
          <Density>
            <Entry unit="kg/m3" ref="18">1778</Entry>
            <Entry unit="kg/m3" ref="23">1760</Entry>
            <Entry unit="kg/m3"></Entry>
            <Entry unit="kg/m3"></Entry>
          </Density>
        
          <!-- Reference ambient temperature (usually room temperature) -->
          <AmbientTemperature>
            <Entry unit="K" ref="18">298</Entry>
            <Entry unit="K"></Entry>
          </AmbientTemperature>
        
          <!-- Melting point of the material -->
          <MeltingTemperature>
            <Entry unit="K" ref="19">415.9</Entry>
            <Entry unit="K" ref="87">414</Entry>
            <Entry unit="K" ref="23">414</Entry>
            <Entry unit="K"></Entry>
          </MeltingTemperature>
        
          <!-- Speed of sound in the material -->
          <SoundSpeed>
            <Entry unit="m/s" ref="20">2300</Entry>
            <Entry unit="m/s"></Entry>
          </SoundSpeed>
        
           <!-- Grüneisen coefficient for EOS/thermal coupling -->
          <GruneisenCoefficient>
            <Entry unit="1" ref="23">1.16</Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
          </GruneisenCoefficient>
        
          <!-- Bulk modulus under isothermal conditions -->
          <IsothermalBulkModulus>
            <Entry unit="Pa" ref="21">8.3E9</Entry>
            <Entry unit="Pa" ref="98">12.3E9</Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
          </IsothermalBulkModulus>
        
          <!-- Cv values used specifically for EOS computations -->
          <SpecificHeatIsochoric>
            <Entry unit="J/kg/K" ref="22">1140</Entry>
            <Entry unit="J/kg/K" ref="23">1090</Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsochoric>
        
           <!-- Polynomial constants for temperature-dependent Cp/Cv formulas -->
          <SpecificHeatConstants>
            <c0 unit="J/kg/K"></c0>
            <c1 unit="J/kg/K"></c1>
            <c2 unit="J/kg/K"></c2>
            <c3 unit="J/kg/K"></c3>
          </SpecificHeatConstants>
        
          <!-- Cp values for isobaric processes -->
          <SpecificHeatIsobaric>
            <Entry unit="J/kg/K" ref="23">1080</Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsobaric>

         <!-- Thermal conductivity values -->
          <ThermalConductivity>
            <Entry unit="W/m/K" ref="23">0.25</Entry>
            <Entry unit="W/m/K"></Entry>
            <Entry unit="W/m/K"></Entry>
            <Entry unit="W/m/K"></Entry>
          </ThermalConductivity>

          <!-- values of thermal expansion -->
          <ThermalExpansion>
            <Entry unit="1/K" ref="23">2.32E-4</Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
          </ThermalExpansion>
        </ThermoMechanical>
      </ElasticModel>
    
      <ElastoPlastic>
        <!-- Shear modulus (rigidity) -->
        <ShearModulus>
          <Entry unit="Pa" ref="20">4E+9</Entry>
          <Entry unit="Pa" ref="23">5E+9</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </ShearModulus>
      
        <!-- Initial yield strength for plastic flow -->
        <YieldStrength>
          <Entry unit="Pa" ref="20">200</Entry>
          <Entry unit="Pa" ref="87">60</Entry>
          <Entry unit="Pa" ref="23">60</Entry>
          <Entry unit="Pa"></Entry>
        </YieldStrength>
      
        <!-- Parameters for the Johnson–Cook plasticity model -->
        <!-- JC constants may be partially left blank if model is not fully used -->
        <JohnsonCookModelConstants>
          <StrainRate unit="1"></StrainRate>
          <ReferenceTemperature unit="K"></ReferenceTemperature>
          <A unit="Pa"></A>
          <B unit="Pa"></B>
          <n unit="1"></n>
          <C unit="1"></C>
          <M unit="1"></M>
          <MeltingTemperature unit="K"></MeltingTemperature>
          <ReferencePressure unit="Pa"></ReferencePressure>
          <A_prime unit="Pa"></A_prime>
          <C_prime unit="1"></C_prime>
        </JohnsonCookModelConstants>
      </ElastoPlastic>
    
      <ReactionModel>
        <!-- Indicates reaction mechanism type -->
        <Kind ref="84">4 steps</Kind>
        <LnZ>
          <Entry index="1" unit="1/s" ref="84">45.59</Entry>
          <Entry index="2" unit="1/s" ref="84">41.95</Entry>
          <Entry index="3" unit="1/s" ref="84">36</Entry>
          <Entry index="4" unit="1/s" ref="84">32</Entry>
        </LnZ>
      
        <!-- Activation energies for each step -->
        <ActivationEnergy>
          <Entry index="1" unit="kcal/mol" ref="84">47.03</Entry>
          <Entry index="2" unit="kcal/mol" ref="84">42.66</Entry>
          <Entry index="3" unit="kcal/mol" ref="84">35.81</Entry>
          <Entry index="4" unit="kcal/mol" ref="84">30.33</Entry>
        </ActivationEnergy>
      
        <!-- Heat released or absorbed during each stage -->
        <HeatRelease>
          <Entry index="1" unit="cal/g" ref="84">50.047</Entry>
          <Entry index="2" unit="cal/g" ref="84">-125.12</Entry>
          <Entry index="3" unit="cal/g" ref="84">-150.14</Entry>
          <Entry index="4" unit="cal/g" ref="84">-1301.24</Entry>
        </HeatRelease>
      
        <!-- Single-step parameters for simplified kinetics -->
        <ReactionModelParameter>
          <Steps unit="1"></Steps>
          <Ea unit="kcal/mol">25.37</Ea>
          <lnZ unit="1/s">30.8</lnZ>
          <Q_R unit="J/g">5861</Q_R>
        
          <!-- Alternative calibrated values -->
          <Ea_new unit="kcal/mol">25.37</Ea_new>
          <lnz_new unit="1/s">30.8</lnz_new>
          <Q_R_new unit="J/g">5861</Q_R_new>
        
          <!-- Original parameters kept for comparison -->
          <Ea_old unit="kcal/mol">39.42</Ea_old>
          <lnZ_old unit="1/s">40.22</lnZ_old>
        </ReactionModelParameter>
      </ReactionModel>

      <EOSModel>
         <!-- Row structure kept generic to support various EOS families -->
        <Row index="1">
          <Kind>Tarver 1997, BM 1999, WR EOS 2008</Kind>
          <K0 unit="Pa"></K0>
          <K0Prime ></K0Prime>
        </Row>
      
        <Row index="2">
          <Kind></Kind>
          <KT0 unit="Pa"></KT0>
          <N unit="1"></N>
        </Row>
      
        <Row index="3">
          <Kind></Kind>
          <Rho unit="kg/m3"></Rho>
          <s unit="1"></s>
          <Cs unit="m/s"></Cs>
          <Gamma unit="1"></Gamma>
          <Cv unit="J/kg/K"></Cv>
        </Row>
      
        <Row index="4">
          <Kind></Kind>
          <GruneisenCoefficient unit="1"></GruneisenCoefficient>
          <A unit="GPa"></A>
          <B unit="GPa"></B>
          <C unit="GPa"></C>
          <D unit="GPa"></D>
          <Rho0 unit="kg/m3"></Rho0>
          <SpecificHeatCv unit="J/kg/K"></SpecificHeatCv>
        </Row>
      
        <Row index="5">
          <Kind></Kind>
          <a unit="1"></a>
          <n unit="1"></n>
          <Vc unit="cm3/g"></Vc>
          <Pc unit="Pa"></Pc>
          <b unit="1"></b>
          <Cv unit="J/kg/K"></Cv>
        </Row>     
        
        <Row index="6">
          <Kind ref="18">JWL</Kind>
          <unreacted>
            <A unit="Pa" ref="18">7320</A>
            <B unit="Pa" ref="18">-0.052654</B>
            <C unit="MJ/kg"></C>
            <R1 unit="1" ref="18">14.1</R1>
            <R2 unit="1" ref="18">1.41</R2>
            <w unit="1" ref="18">0.8938</w>
            <Cv unit="MJ/kg/K" ref="18">0.00002704</Cv>
            <E0 unit="MJ/kg"></E0>
          </unreacted>
        
          <reacted>
            <A unit="Pa" ref="18">10.506</A>
            <B unit="Pa" ref="18">0.93391</B>
            <C unit="MJ/kg"></C>
            <R1 unit="1" ref="18">6</R1>
            <R2 unit="1" ref="18">2.6</R2>
            <w unit="1" ref="18">0.57</w>
            <Cv unit="MJ/kg/K" ref="18">0.00001</Cv>
            <E0 unit="MJ/kg" ref="18">0.10892</E0>
          </reacted>
        </Row>
      </EOSModel>  
    </Model>
  </Category>
</Material>
