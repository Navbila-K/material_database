<?xml version="1.0" encoding="UTF-8"?>

<!--
=========================================================
                MATERIAL DESCRIPTION FILE
=========================================================
This XML defines a single material along with:
- metadata (id, name, author, date, version)
- categories of physical models (thermal, mechanical, plasticity, reaction, EOS)
- data stored as entries with units separated into attributes
- fully explicit structure, future-proof and backward compatible
=========================================================
-->
<Material>
  <!-- The metadata identifies the material and makes files traceable. -->
  <Metadata>
  <Id>TNT-001</Id>
  <Name>TNT</Name>
  <Author></Author>
  <Date></Date>
  <Version meaning="schema_version">0.0.0-D05AA897</Version><!-- Indicates the version of the XML schema used for defining this material structure -->
  </Metadata>
    
  <Category>
    <Property>

            <!-- Phase information (enumeration: gas, liquid, solid, plasma) -->
            <Phase>
                <State>solid</State>
            </Phase>

            <!-- Thermal properties -->
            <Thermal>
                <!-- specific heat at constant pressure (J/kg·K) -->
                <Cp unit="J/kg/K">
                    <Entry ref="23">1604</Entry>
                </Cp>

                <!-- specific heat at constant volume (J/kg·K) -->
                <Cv unit="J/kg/K">
                    <Entry ref="22">1270</Entry>
                </Cv>
            </Thermal>

            <!-- Mechanical properties -->
            <Mechanical>
                <!-- density in kg/m^3 -->
                <Density unit="kg/m^3">
                    <Entry ref="22">1654</Entry>
                </Density>

                <!--viscosity in Pa·s -->
                <Viscosity unit="Pa*s"> <!-- or we can use cP (centipoise) as alt-->
                    <Entry ref="0">0.0000</Entry>
                </Viscosity>
            </Mechanical>

        </Property>

    <Model>
      <ElasticModel>
        <ThermoMechanical>
          <!-- Density values used in thermo-mechanical calculations -->
          <Density>
            <Entry unit="kg/m3" ref="22">1654</Entry>
            <Entry unit="kg/m3"></Entry>
            <Entry unit="kg/m3"></Entry>
            <Entry unit="kg/m3"></Entry>
          </Density>
        
          <!-- Reference ambient temperature (usually room temperature) -->
          <AmbientTemperature>
            <Entry unit="K" ref="23">298</Entry>
            <Entry unit="K"></Entry>
          </AmbientTemperature>
        
          <!-- Melting point of the material -->
          <MeltingTemperature>
            <Entry unit="K" ref="23">358</Entry>
            <Entry unit="K" ref="87">354</Entry>
            <Entry unit="K" ref="105">354</Entry>
            <Entry unit="K"></Entry>
          </MeltingTemperature>
        
          <!-- Speed of sound in the material -->
          <SoundSpeed>
            <Entry unit="m/s" ref="23">1550</Entry>
            <Entry unit="m/s"></Entry>
          </SoundSpeed>
        
           <!-- Grüneisen coefficient for EOS/thermal coupling -->
          <GruneisenCoefficient>
            <Entry unit="1" ref="23">0.05</Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
          </GruneisenCoefficient>
        
          <!-- Bulk modulus under isothermal conditions -->
          <IsothermalBulkModulus>
            <Entry unit="Pa" ref="56">8.52E9</Entry>
            <Entry unit="Pa" ref="100">8.52E9</Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
          </IsothermalBulkModulus>
        
          <!-- Cv values used specifically for EOS computations -->
          <SpecificHeatIsochoric>
            <Entry unit="J/kg/K" ref="22">1270</Entry>
            <Entry unit="J/kg/K"></Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsochoric>
        
           <!-- Polynomial constants for temperature-dependent Cp/Cv formulas -->
          <SpecificHeatConstants>
            <c0 unit="J/kg/K" ref="58">0.0045077</c0>
            <c1 unit="J/kg/K" ref="58">90248.6</c1>
            <c2 unit="J/kg/K" ref="58">-67242857</c2>
            <c3 unit="J/kg/K" ref="58">18156981000</c3>
          </SpecificHeatConstants>
        
          <!-- Cp values for isobaric processes -->
          <SpecificHeatIsobaric>
            <Entry unit="J/kg/K" ref="23">1604</Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsobaric>

         <!-- Thermal conductivity values -->
          <ThermalConductivity>
            <Entry unit="W/m/K" ref="57">2.6</Entry>
            <Entry unit="W/m/K" ref="105">0.259</Entry>
            <Entry unit="W/m/K" ref="57">0.259</Entry>
            <Entry unit="W/m/K"></Entry>
          </ThermalConductivity>

          <!-- values of thermal expansion -->
          <ThermalExpansion>
            <Entry unit="1/K" ref="23">7E-4</Entry>
            <Entry unit="1/K" ref="104">0.000046</Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
          </ThermalExpansion>
        </ThermoMechanical>
      </ElasticModel>
    
      <ElastoPlastic>
        <!-- Shear modulus (rigidity) -->
        <ShearModulus>
          <Entry unit="Pa" ref="59">2.48E+9</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </ShearModulus>
      
        <!-- Initial yield strength for plastic flow -->
        <YieldStrength>
          <Entry unit="Pa" ref="60">20</Entry>
          <Entry unit="Pa" ref="87">34</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </YieldStrength>
      
        <!-- Parameters for the Johnson–Cook plasticity model -->
        <!-- JC constants may be partially left blank if model is not fully used -->
        <JohnsonCookModelConstants>
          <StrainRate unit="1" ref="61">50</StrainRate>
          <ReferenceTemperature unit="K"></ReferenceTemperature>
          <A unit="Pa" ref="61">375E+6</A>
          <B unit="Pa" ref="61">600E+6</B>
          <n unit="1" ref="61">0.07</n>
          <C unit="1" ref="61">0.09</C>
          <M unit="1" ref="61">0.35</M>
          <MeltingTemperature unit="K"></MeltingTemperature>
          <ReferencePressure unit="Pa"></ReferencePressure>
          <A_prime unit="Pa"></A_prime>
          <C_prime unit="1"></C_prime>
        </JohnsonCookModelConstants>
      </ElastoPlastic>
    
      <ReactionModel>
        <!-- Indicates reaction mechanism type -->
        <Kind ref="5">3-step</Kind>
        <LnZ>
          <Entry index="1" unit="1/s" ref="57">35</Entry>
          <Entry index="2" unit="1/s" ref="57">26</Entry>
          <Entry index="3" unit="1/s" ref="57">26.2</Entry>
          <Entry index="4" unit="1/s" ></Entry>
        </LnZ>
      
        <!-- Activation energies for each step -->
        <ActivationEnergy>
          <Entry index="1" unit="kcal/mol" ref="57">44</Entry>
          <Entry index="2" unit="kcal/mol" ref="57">34.5</Entry>
          <Entry index="3" unit="kcal/mol" ref="57">33.5</Entry>
          <Entry index="4" unit="kcal/mol"></Entry>
        </ActivationEnergy>
      
        <!-- Heat released or absorbed during each stage -->
        <HeatRelease>
          <Entry index="1" unit="cal/g" ref="57">-30</Entry>
          <Entry index="2" unit="cal/g" ref="57">900</Entry>
          <Entry index="3" unit="cal/g" ref="57">930</Entry>
          <Entry index="4" unit="cal/g"></Entry>
        </HeatRelease>
      
        <!-- Single-step parameters for simplified kinetics -->
        <ReactionModelParameter>
          <Steps unit="1"></Steps>
          <Ea unit="kcal/mol">27.53</Ea>
          <lnZ unit="1/s">25.15</lnZ>
          <Q_R unit="J/g">5861</Q_R>
        
          <!-- Alternative calibrated values -->
          <Ea_new unit="kcal/mol">27.53</Ea_new>
          <lnz_new unit="1/s">25.15</lnz_new>
          <Q_R_new unit="J/g">5861</Q_R_new>
        
          <!-- Original parameters kept for comparison -->
          <Ea_old unit="kcal/mol">37.2</Ea_old>
          <lnZ_old unit="1/s">30.64</lnZ_old>
        </ReactionModelParameter>
      </ReactionModel>

      <EOSModel>
         <!-- Row structure kept generic to support various EOS families -->
        <Row index="1">
          <Kind ref="56">Murnaghan</Kind>
          <K0 unit="Pa" ref="56">8.52</K0>
          <K0Prime  ref="56">8</K0Prime>
        </Row>
      
        <Row index="2">
          <Kind></Kind>
          <KT0 unit="Pa"></KT0>
          <N unit="1"></N>
        </Row>
      
        <Row index="3">
          <Kind></Kind>
          <Rho unit="kg/m3"></Rho>
          <s unit="1"></s>
          <Cs unit="m/s"></Cs>
          <Gamma unit="1"></Gamma>
          <Cv unit="J/kg/K"></Cv>
        </Row>
      
        <Row index="4">
          <Kind></Kind>
          <GruneisenCoefficient unit="1"></GruneisenCoefficient>
          <A unit="GPa"></A>
          <B unit="GPa"></B>
          <C unit="GPa"></C>
          <D unit="GPa"></D>
          <Rho0 unit="kg/m3"></Rho0>
          <SpecificHeatCv unit="J/kg/K"></SpecificHeatCv>
        </Row>
      
        <Row index="5">
          <Kind>WSD</Kind>
          <a unit="1"></a>
          <n unit="1"></n>
          <Vc unit="cm3/g"></Vc>
          <Pc unit="Pa"></Pc>
          <b unit="1"></b>
          <Cv unit="J/kg/K"></Cv>
        </Row>     
        
        <Row index="6">
          <Kind ref="62">JWL</Kind>
          <unreacted>
            <A unit="Pa" ref="62">17101</A>
            <B unit="Pa" ref="62">-3.745</B>
            <C unit="MJ/kg"></C>
            <R1 unit="1" ref="62">9.8</R1>
            <R2 unit="1" ref="62">0.98</R2>
            <w unit="1" ref="62">0.5675</w>
            <Cv unit="MJ/kg/K" ref="62">0.00270386</Cv>
            <E0 unit="MJ/kg" ref="62">0</E0>
          </unreacted>
        
          <reacted>
            <A unit="Pa" ref="62">673.1</A>
            <B unit="Pa" ref="62">21.988</B>
            <C unit="MJ/kg"></C>
            <R1 unit="1" ref="62">5.4</R1>
            <R2 unit="1" ref="62">1.8</R2>
            <w unit="1" ref="62">0.3</w>
            <Cv unit="MJ/kg/K" ref="62">0.001</Cv>
            <E0 unit="MJ/kg" ref="62">7</E0>
          </reacted>
        </Row>
      </EOSModel>  
    </Model>
  </Category>
</Material>