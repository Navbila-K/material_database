<?xml version="1.0" encoding="UTF-8"?>

<!--
=========================================================
                MATERIAL DESCRIPTION FILE
=========================================================
This XML defines a single material along with:
- metadata (id, name, author, date, version)
- categories of physical models (thermal, mechanical, plasticity, reaction, EOS)
- data stored as entries with units separated into attributes
- fully explicit structure, future-proof and backward compatible
=========================================================
-->
<Material>
  <!-- The metadata identifies the material and makes files traceable. -->
  <Metadata>
  <Id>TATB-001</Id>
  <Name>TATB</Name>
  <Author></Author>
  <Date></Date>
  <Version meaning="schema_version">0.0.0-60E1095E</Version><!-- Indicates the version of the XML schema used for defining this material structure -->
  </Metadata>
    
  <Category>
    <Property>

            <!-- Phase information (enumeration: gas, liquid, solid, plasma) -->
            <Phase>
                <State>solid</State>
            </Phase>

            <!-- Thermal properties -->
            <Thermal>
                <!-- specific heat at constant pressure (J/kg·K) -->
                <Cp unit="J/kg/K">
                    <Entry ref="23">1000</Entry>
                </Cp>

                <!-- specific heat at constant volume (J/kg·K) -->
                <Cv unit="J/kg/K">
                    <Entry ref="1">837</Entry>
                </Cv>
            </Thermal>

            <!-- Mechanical properties -->
            <Mechanical>
                <!-- density in kg/m^3 -->
                <Density unit="kg/m^3">
                    <Entry ref="1">1900</Entry>
                    <Entry ref="102">1915</Entry>
                </Density>

                <!--viscosity in Pa·s -->
                <Viscosity unit="Pa*s"> <!-- or we can use cP (centipoise) as alt-->
                    <Entry ref="0">0.0000</Entry>
                </Viscosity>
            </Mechanical>

        </Property>

    <Model>
      <ElasticModel>
        <ThermoMechanical>
          <!-- Density values used in thermo-mechanical calculations -->
          <Density>
            <Entry unit="kg/m3" ref="1">1900</Entry>
            <Entry unit="kg/m3" ref="102">1915</Entry>
            <Entry unit="kg/m3"></Entry>
            <Entry unit="kg/m3"></Entry>
          </Density>
        
          <!-- Reference ambient temperature (usually room temperature) -->
          <AmbientTemperature>
            <Entry unit="K" ref="1">298</Entry>
            <Entry unit="K"></Entry>
          </AmbientTemperature>
        
          <!-- Melting point of the material -->
          <MeltingTemperature>
            <Entry unit="K" ref="1">623</Entry>
            <Entry unit="K"></Entry>
            <Entry unit="K"></Entry>
            <Entry unit="K"></Entry>
          </MeltingTemperature>
        
          <!-- Speed of sound in the material -->
          <SoundSpeed>
            <Entry unit="m/s" ref="23">1430</Entry>
            <Entry unit="m/s"></Entry>
          </SoundSpeed>
        
           <!-- Grüneisen coefficient for EOS/thermal coupling -->
          <GruneisenCoefficient>
            <Entry unit="1" ref="3">0.8168</Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
          </GruneisenCoefficient>
        
          <!-- Bulk modulus under isothermal conditions -->
          <IsothermalBulkModulus>
            <Entry unit="Pa" ref="4">1.71eE10</Entry>
            <Entry unit="Pa" ref="98">14.7E9</Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
          </IsothermalBulkModulus>
        
          <!-- Cv values used specifically for EOS computations -->
          <SpecificHeatIsochoric>
            <Entry unit="J/kg/K" ref="1">837</Entry>
            <Entry unit="J/kg/K"></Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsochoric>
        
           <!-- Polynomial constants for temperature-dependent Cp/Cv formulas -->
          <SpecificHeatConstants>
            <c0 unit="J/kg/K"></c0>
            <c1 unit="J/kg/K"></c1>
            <c2 unit="J/kg/K"></c2>
            <c3 unit="J/kg/K"></c3>
          </SpecificHeatConstants>
        
          <!-- Cp values for isobaric processes -->
          <SpecificHeatIsobaric>
            <Entry unit="J/kg/K" ref="23">1000</Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsobaric>

         <!-- Thermal conductivity values -->
          <ThermalConductivity>
            <Entry unit="W/m/K" ref="82">0.53</Entry>
            <Entry unit="W/m/K" ref="57">0.79</Entry>
            <Entry unit="W/m/K"></Entry>
            <Entry unit="W/m/K"></Entry>
          </ThermalConductivity>

          <!-- values of thermal expansion -->
          <ThermalExpansion>
            <Entry unit="1/K" ref="23">0.995E-4</Entry>
            <Entry unit="1/K" ref="98">1.9E-4</Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
          </ThermalExpansion>
        </ThermoMechanical>
      </ElasticModel>
    
      <ElastoPlastic>
        <!-- Shear modulus (rigidity) -->
        <ShearModulus>
          <Entry unit="Pa" ref="1">7E9</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </ShearModulus>
      
        <!-- Initial yield strength for plastic flow -->
        <YieldStrength>
          <Entry unit="Pa" ref="1">100</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </YieldStrength>
      
        <!-- Parameters for the Johnson–Cook plasticity model -->
        <!-- JC constants may be partially left blank if model is not fully used -->
        <JohnsonCookModelConstants>
          <StrainRate unit="1"></StrainRate>
          <ReferenceTemperature unit="K"></ReferenceTemperature>
          <A unit="Pa"></A>
          <B unit="Pa"></B>
          <n unit="1"></n>
          <C unit="1"></C>
          <M unit="1"></M>
          <MeltingTemperature unit="K"></MeltingTemperature>
          <ReferencePressure unit="Pa"></ReferencePressure>
          <A_prime unit="Pa"></A_prime>
          <C_prime unit="1"></C_prime>
        </JohnsonCookModelConstants>
      </ElastoPlastic>
    
      <ReactionModel>
        <!-- Indicates reaction mechanism type -->
        <Kind ref="5">3-step</Kind>
        <LnZ>
          <Entry index="1" unit="1/s" ref="5">48</Entry>
          <Entry index="2" unit="1/s" ref="5">29.8</Entry>
          <Entry index="3" unit="1/s" ref="5">26.8</Entry>
          <Entry index="4" unit="1/s" ></Entry>
        </LnZ>
      
        <!-- Activation energies for each step -->
        <ActivationEnergy>
          <Entry index="1" unit="kcal/mol" ref="5">60</Entry>
          <Entry index="2" unit="kcal/mol" ref="5">42</Entry>
          <Entry index="3" unit="kcal/mol" ref="5">33.8</Entry>
          <Entry index="4" unit="kcal/mol"></Entry>
        </ActivationEnergy>
      
        <!-- Heat released or absorbed during each stage -->
        <HeatRelease>
          <Entry index="1" unit="cal/g" ref="5">50</Entry>
          <Entry index="2" unit="cal/g" ref="5">50</Entry>
          <Entry index="3" unit="cal/g" ref="5">-700</Entry>
          <Entry index="4" unit="cal/g"></Entry>
        </HeatRelease>
      
        <!-- Single-step parameters for simplified kinetics -->
        <ReactionModelParameter>
          <Steps unit="1"></Steps>
          <Ea unit="kcal/mol">38.29</Ea>
          <lnZ unit="1/s">28.76</lnZ>
          <Q_R unit="J/g">5861</Q_R>
        
          <!-- Alternative calibrated values -->
          <Ea_new unit="kcal/mol">37.27</Ea_new>
          <lnz_new unit="1/s">27.8</lnz_new>
          <Q_R_new unit="J/g">4103.26</Q_R_new>
        
          <!-- Original parameters kept for comparison -->
          <Ea_old unit="kcal/mol">38.29</Ea_old>
          <lnZ_old unit="1/s">28.76</lnZ_old>
        </ReactionModelParameter>
      </ReactionModel>

      <EOSModel>
         <!-- Row structure kept generic to support various EOS families -->
        <Row index="1">
          <Kind ref="4">BM</Kind>
          <K0 unit="Pa" ref="4">1.71</K0>
          <K0Prime  ref="4">8.1</K0Prime>
        </Row>
      
        <Row index="2">
          <Kind></Kind>
          <KT0 unit="Pa"></KT0>
          <N unit="1"></N>
        </Row>
      
        <Row index="3">
          <Kind></Kind>
          <Rho unit="kg/m3"></Rho>
          <s unit="1"></s>
          <Cs unit="m/s"></Cs>
          <Gamma unit="1"></Gamma>
          <Cv unit="J/kg/K"></Cv>
        </Row>
      
        <Row index="4">
          <Kind></Kind>
          <GruneisenCoefficient unit="1"></GruneisenCoefficient>
          <A unit="GPa"></A>
          <B unit="GPa"></B>
          <C unit="GPa"></C>
          <D unit="GPa"></D>
          <Rho0 unit="kg/m3"></Rho0>
          <SpecificHeatCv unit="J/kg/K"></SpecificHeatCv>
        </Row>
      
        <Row index="5">
          <Kind ref="3">WSD</Kind>
          <a unit="1" ref="3">0.8592</a>
          <n unit="1" ref="3">2.521</n>
          <Vc unit="cm3/g" ref="3">0.9884</Vc>
          <Pc unit="Pa" ref="3">1.307</Pc>
          <b unit="1" ref="3">0.58</b>
          <Cv unit="J/kg/K" ref="3">725</Cv>
        </Row>     
        
        <Row index="6">
          <Kind ref="28">JWL</Kind>
          <unreacted>
            <A unit="Pa" ref="28">6.3207E10</A>
            <B unit="Pa" ref="28">-4.472E9</B>
            <C unit="MJ/kg" ref="28">6.9</C>
            <R1 unit="1" ref="28">11.3</R1>
            <R2 unit="1" ref="28">1.13</R2>
            <w unit="1" ref="28">0.8938</w>
            <Cv unit="MJ/kg/K" ref="28">2.487E-3</Cv>
            <E0 unit="MJ/kg"></E0>
          </unreacted>
        
          <reacted>
            <A unit="Pa" ref="28">1.36177E12</A>
            <B unit="Pa" ref="28">7.199E10</B>
            <C unit="MJ/kg" ref="28">-0.246929</C>
            <R1 unit="1" ref="28">6.2</R1>
            <R2 unit="1" ref="28">2.2</R2>
            <w unit="1" ref="28">0.5</w>
            <Cv unit="MJ/kg/K" ref="28">1E-3</Cv>
            <E0 unit="MJ/kg" ref="28">0</E0>
          </reacted>
        </Row>
      </EOSModel>  
    </Model>
  </Category>
</Material>