<?xml version="1.0" encoding="UTF-8"?>

<!--
=========================================================
                MATERIAL DESCRIPTION FILE
=========================================================
This XML defines a single material along with:
- metadata (id, name, author, date, version)
- categories of physical models (thermal, mechanical, plasticity, reaction, EOS)
- data stored as entries with units separated into attributes
- fully explicit structure, future-proof and backward compatible
=========================================================
-->
<Material>
  <!-- The metadata identifies the material and makes files traceable. -->
  <Metadata>
  <Id>HNS-001</Id>
  <Name>HNS</Name>
  <Author></Author>
  <Date></Date>
  <Version meaning="schema_version">0.0.0-8620F1A2</Version><!-- Indicates the version of the XML schema used for defining this material structure -->
  </Metadata>
    
  <Category>
    <Property>

            <!-- Phase information (enumeration: gas, liquid, solid, plasma) -->
            <Phase>
                <State>solid</State>
            </Phase>

            <!-- Thermal properties -->
            <Thermal>
                <!-- specific heat at constant pressure (J/kg·K) -->
                <Cp unit="J/kg/K">
                    <Entry ref="77">1282</Entry>
                </Cp>

                <!-- specific heat at constant volume (J/kg·K) -->
                <Cv unit="J/kg/K">
                    <Entry ref="77">828</Entry>
                </Cv>
            </Thermal>

            <!-- Mechanical properties -->
            <Mechanical>
                <!-- density in kg/m^3 -->
                <Density unit="kg/m^3">
                    <Entry ref="73">1740</Entry>
                </Density>

                <!--viscosity in Pa·s -->
                <Viscosity unit="Pa*s"> <!-- or we can use cP (centipoise) as alt-->
                    <Entry ref="0">0.0000</Entry>
                </Viscosity>
            </Mechanical>

        </Property>

    <Model>
      <ElasticModel>
        <ThermoMechanical>
          <!-- Density values used in thermo-mechanical calculations -->
          <Density>
            <Entry unit="kg/m3" ref="73">1740</Entry>
            <Entry unit="kg/m3"></Entry>
            <Entry unit="kg/m3"></Entry>
            <Entry unit="kg/m3"></Entry>
          </Density>
        
          <!-- Reference ambient temperature (usually room temperature) -->
          <AmbientTemperature>
            <Entry unit="K" ref="72">298</Entry>
            <Entry unit="K"></Entry>
          </AmbientTemperature>
        
          <!-- Melting point of the material -->
          <MeltingTemperature>
            <Entry unit="K" ref="72">588</Entry>
            <Entry unit="K" ref="87">588</Entry>
            <Entry unit="K"></Entry>
            <Entry unit="K"></Entry>
          </MeltingTemperature>
        
          <!-- Speed of sound in the material -->
          <SoundSpeed>
            <Entry unit="m/s" ref="76">2762</Entry>
            <Entry unit="m/s"></Entry>
          </SoundSpeed>
        
           <!-- Grüneisen coefficient for EOS/thermal coupling -->
          <GruneisenCoefficient>
            <Entry unit="1" ref="72">1.625</Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
            <Entry unit="1"></Entry>
          </GruneisenCoefficient>
        
          <!-- Bulk modulus under isothermal conditions -->
          <IsothermalBulkModulus>
            <Entry unit="Pa" ref="78">11.2E9</Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
            <Entry unit="Pa"></Entry>
          </IsothermalBulkModulus>
        
          <!-- Cv values used specifically for EOS computations -->
          <SpecificHeatIsochoric>
            <Entry unit="J/kg/K" ref="77">828</Entry>
            <Entry unit="J/kg/K"></Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsochoric>
        
           <!-- Polynomial constants for temperature-dependent Cp/Cv formulas -->
          <SpecificHeatConstants>
            <c0 unit="J/kg/K"></c0>
            <c1 unit="J/kg/K"></c1>
            <c2 unit="J/kg/K"></c2>
            <c3 unit="J/kg/K"></c3>
          </SpecificHeatConstants>
        
          <!-- Cp values for isobaric processes -->
          <SpecificHeatIsobaric>
            <Entry unit="J/kg/K" ref="77">1282</Entry>
            <Entry unit="J/kg/K"></Entry>
          </SpecificHeatIsobaric>

         <!-- Thermal conductivity values -->
          <ThermalConductivity>
            <Entry unit="W/m/K" ref="75">0.7106</Entry>
            <Entry unit="W/m/K"></Entry>
            <Entry unit="W/m/K"></Entry>
            <Entry unit="W/m/K"></Entry>
          </ThermalConductivity>

          <!-- values of thermal expansion -->
          <ThermalExpansion>
            <Entry unit="1/K" ref="74">0.00016725</Entry>
            <Entry unit="1/K" ref="98">0.00021</Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
            <Entry unit="1/K"></Entry>
          </ThermalExpansion>
        </ThermoMechanical>
      </ElasticModel>
    
      <ElastoPlastic>
        <!-- Shear modulus (rigidity) -->
        <ShearModulus>
          <Entry unit="Pa" ref="72">5.686E+9</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </ShearModulus>
      
        <!-- Initial yield strength for plastic flow -->
        <YieldStrength>
          <Entry unit="Pa" ref="72">140</Entry>
          <Entry unit="Pa" ref="87">140</Entry>
          <Entry unit="Pa"></Entry>
          <Entry unit="Pa"></Entry>
        </YieldStrength>
      
        <!-- Parameters for the Johnson–Cook plasticity model -->
        <!-- JC constants may be partially left blank if model is not fully used -->
        <JohnsonCookModelConstants>
          <StrainRate unit="1"></StrainRate>
          <ReferenceTemperature unit="K"></ReferenceTemperature>
          <A unit="Pa" ref="80">50E+6</A>
          <B unit="Pa" ref="80">2000E+6</B>
          <n unit="1" ref="80">0.6</n>
          <C unit="1" ref="80">0.004</C>
          <M unit="1" ref="80">0.4</M>
          <MeltingTemperature unit="K" ref="80">1673</MeltingTemperature>
          <ReferencePressure unit="Pa"></ReferencePressure>
          <A_prime unit="Pa"></A_prime>
          <C_prime unit="1"></C_prime>
        </JohnsonCookModelConstants>
      </ElastoPlastic>
    
      <ReactionModel>
        <!-- Indicates reaction mechanism type -->
        <Kind ref="85">3steps</Kind>
        <LnZ>
          <Entry index="1" unit="1/s" ref="85">15.5</Entry>
          <Entry index="2" unit="1/s" ref="85">17.8</Entry>
          <Entry index="3" unit="1/s" ref="85">64</Entry>
          <Entry index="4" unit="1/s"></Entry>
        </LnZ>
      
        <!-- Activation energies for each step -->
        <ActivationEnergy>
          <Entry index="1" unit="kcal/mol" ref="85">51.91</Entry>
          <Entry index="2" unit="kcal/mol" ref="85">55.98</Entry>
          <Entry index="3" unit="kcal/mol" ref="85">170.33</Entry>
          <Entry index="4" unit="kcal/mol"></Entry>
        </ActivationEnergy>
      
        <!-- Heat released or absorbed during each stage -->
        <HeatRelease>
          <Entry index="1" unit="cal/g"></Entry>
          <Entry index="2" unit="cal/g"></Entry>
          <Entry index="3" unit="cal/g"></Entry>
          <Entry index="4" unit="cal/g"></Entry>
        </HeatRelease>
      
        <!-- Single-step parameters for simplified kinetics -->
        <ReactionModelParameter>
          <Steps unit="1"></Steps>
          <Ea unit="kcal/mol"></Ea>
          <lnZ unit="1/s"></lnZ>
          <Q_R unit="J/g"></Q_R>
        
          <!-- Alternative calibrated values -->
          <Ea_new unit="kcal/mol"></Ea_new>
          <lnz_new unit="1/s"></lnz_new>
          <Q_R_new unit="J/g"></Q_R_new>
        
          <!-- Original parameters kept for comparison -->
          <Ea_old unit="kcal/mol"></Ea_old>
          <lnZ_old unit="1/s"></lnZ_old>
        </ReactionModelParameter>
      </ReactionModel>

      <EOSModel>
         <!-- Row structure kept generic to support various EOS families -->
        <Row index="1">
          <Kind ref="78">BM</Kind>
          <K0 unit="Pa" ref="78">11.2E9</K0>
          <K0Prime  ref="78">6.2</K0Prime>
        </Row>
      
        <Row index="2">
          <Kind></Kind>
          <KT0 unit="Pa"></KT0>
          <N unit="1"></N>
        </Row>
      
        <Row index="3">
          <Kind></Kind>
          <Rho unit="kg/m3"></Rho>
          <s unit="1"></s>
          <Cs unit="m/s"></Cs>
          <Gamma unit="1"></Gamma>
          <Cv unit="J/kg/K"></Cv>
        </Row>
      
        <Row index="4">
          <Kind></Kind>
          <GruneisenCoefficient unit="1"></GruneisenCoefficient>
          <A unit="GPa"></A>
          <B unit="GPa"></B>
          <C unit="GPa"></C>
          <D unit="GPa"></D>
          <Rho0 unit="kg/m3"></Rho0>
          <SpecificHeatCv unit="J/kg/K"></SpecificHeatCv>
        </Row>
      
        <Row index="5">
          <Kind></Kind>
          <a unit="1"></a>
          <n unit="1"></n>
          <Vc unit="cm3/g"></Vc>
          <Pc unit="Pa"></Pc>
          <b unit="1"></b>
          <Cv unit="J/kg/K"></Cv>
        </Row>     
        
        <Row index="6">
          <Kind ref="79">JWL</Kind>
          <unreacted>
            <A unit="Pa" ref="79">33180</A>
            <B unit="Pa" ref="79">-2.5154</B>
            <C unit="MJ/kg"></C>
            <R1 unit="1" ref="79">11.5</R1>
            <R2 unit="1" ref="79">1.15</R2>
            <w unit="1" ref="79">0.5675</w>
            <Cv unit="MJ/kg/K" ref="79">0.00002704</Cv>
            <E0 unit="MJ/kg"></E0>
          </unreacted>
        
          <reacted>
            <A unit="Pa" ref="79">536.25</A>
            <B unit="Pa" ref="79">27.02</B>
            <C unit="MJ/kg"></C>
            <R1 unit="1" ref="79">5.4</R1>
            <R2 unit="1" ref="79">1.8</R2>
            <w unit="1" ref="79">0.45</w>
            <Cv unit="MJ/kg/K" ref="79">0.00001</Cv>
            <E0 unit="MJ/kg" ref="79">0.07</E0>
          </reacted>
        </Row>
      </EOSModel>  
    </Model>
  </Category>
</Material>
